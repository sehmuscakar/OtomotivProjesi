
@model List<DataAccessLayer.Entities.ContactCity>

<div class="col-lg-4">

    @foreach (var item in Model)
    {


        <div class="info">
            <div class="address">
                <i class="bi bi-geo-alt"></i>
                <h4>Adresimiz</h4>
                <p>@item.Address</p>
            </div>

            <div class="email">
                <i class="bi bi-envelope"></i>
                <h4>Mail</h4>
                <p>@item.MailCity</p>
            </div>

            <div class="phone">
                <i class="bi bi-phone"></i>
                <h4>Telefon</h4>
                <p>@item.Phone</p>
            </div>

        </div>
    }
</div>