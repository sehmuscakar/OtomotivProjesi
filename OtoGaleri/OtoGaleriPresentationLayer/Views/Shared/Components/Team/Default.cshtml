@model List<DataAccessLayer.Entities.Team>
<section id="team" class="team ">
    <div class="container">
        <div class="section-title">
            <h2>Takım Arkadaşlarımız</h2>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                
       
            <div class="col-lg-6">
                <div class="member d-flex align-items-start">
                    <div class="pic"><img src="~/Img/@item.Image" class="img-fluid" alt=""></div>
                    <div class="member-info">
                        <h4>@item.FullName</h4>
                        <span>@item.Duty</span>
                        <div class="social">
                            <a href="@item.Twitter"><i class="ri-twitter-fill"></i></a>
                            <a href="@item.Facebook"><i class="ri-facebook-fill"></i></a>
                            <a href="@item.Instegram"><i class="ri-instagram-fill"></i></a>
                            <a href="@item.LinkedIn"> <i class="ri-linkedin-box-fill"></i> </a>
                        </div>
                    </div>
                </div>
            </div>
            }
       

        </div>

    </div>
</section><!-- End Team Section -->